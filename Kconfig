config RESOLUTION_X
	int "Display Width"
	default 256

config RESOLUTION_Y
	int "Display Height"
	default 128

config TARGET_RENDER_FPS
	int "FPS of rendering"
	default 30

config TARGET_PROCESS_FPS
	int "FPS of processing logic"
	default 30

config LOG_PERFORMANCE
	bool "enable logging render times, this can cause render lag"
	default y

config MAX_OBJECTS
	int "Maximum amount of objects"
	default 512

config MAX_LIGHTS
	int "Maximum amount of object lights"
	default 16

rsource "lib/Kconfig"
